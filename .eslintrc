{
	"parser": "babel-eslint",
	"plugins": ["react"],
	"rules": {
		"import/extensions": 0,
		"import/no-extraneous-dependencies": 0,
		"import/no-unresolved": 0,
		"react/jsx-filename-extension": 0,
		"linebreak-style": 0,
		"no-case-declarations": 0,
		"no-console": 0,
		"react/prop-types": 0,
		"react/display-name": 0
	}
}
